{
  "name": "@chubbyts/chubbyts-dic-types",
  "type": "module",
  "version": "1.3.0",
  "description": "Dependency injection container (DIC) types, PSR-11 inspired.",
  "keywords": [
    "chubbyts",
    "dic",
    "dependency injection container",
    "dependency injection",
    "container",
    "PSR-11"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-dic-types",
  "scripts": {
    "build": "node ./build.mjs",
    "cs-fix": "prettier --write src",
    "cs": "prettier --check src",
    "infection": "stryker run",
    "lint-fix": "eslint src eslint.config.mjs --fix",
    "lint": "eslint src",
    "prepare": "npm run build"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./*.d.ts",
      "require": "./*.cjs",
      "import": "./*.mjs",
      "default": "./*.mjs"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {},
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^3.0.1",
    "@chubbyts/chubbyts-packaging": "^3.0.0",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
